    "js-cookie": "2.2.0",
    "jsencrypt": "^3.0.0",
    "lodash": "^4.17.20",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "8.0.0",
    "vue": "^2.7.16",
    "vue-count-to": "^1.0.13",
    "vue-cropper": "^0.5.6",
    "vue-router": "^3.6.5",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
